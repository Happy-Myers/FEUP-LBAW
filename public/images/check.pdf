                            if(curType !== null && curType === 'checkbox') {
                                aCheck.push(aElms[i]);
                            }
                        }

                        return aCheck;
                    }
                }



                function checkChecked(aCheck, callBack) {
                    var checked = 0;
                    for(var i=0, len=aCheck.length; i < len; i++) {
                        if(aCheck[i].checked) {
                            checked++;
                        }
                    }
                    callBack(checked, aCheck.length); 
                }

                function onClickForm(event) {
