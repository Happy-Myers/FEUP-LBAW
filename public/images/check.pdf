                            }
                            else {
                                document.getElementById('out').classList.remove("done");
                                document.getElementById('out').classList.remove("almost");                                
                            }
                        });
                    }

                    
                }

                var aCheckbox = getCheck('myf');
                if(!aCheckbox) {
